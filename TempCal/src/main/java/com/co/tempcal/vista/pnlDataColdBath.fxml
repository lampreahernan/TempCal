<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="525.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.co.tempcal.vista.TemperatureInfoController">
   <children>
      <Label layoutX="53.0" layoutY="14.0" text="Soak sensor in the cold bath. When LCD is stable adjust cold zero (R13)" textAlignment="CENTER" textOverrun="CLIP">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <HBox layoutX="125.0" layoutY="88.0">
         <children>
            <Label text="Cold Bath Temperature " />
            <TextField fx:id="txtColdBathTemp" />
         </children>
      </HBox>
      <Button fx:id="btnNext" layoutX="372.0" layoutY="151.0" mnemonicParsing="false" onAction="#handleNext" prefHeight="25.0" prefWidth="102.0" text="Next" />
      <HBox layoutX="21.0" layoutY="151.0">
         <children>
            <Label prefHeight="25.0" prefWidth="80.0" text="Time Elapsed" />
            <TextField fx:id="txtTimeElapsedColdBath" editable="false" prefHeight="25.0" prefWidth="50.0" />
         </children>
      </HBox>
      <Label layoutX="188.0" layoutY="31.0" text="to equal bath temperature">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
